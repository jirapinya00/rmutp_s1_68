generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Profile {
  id        String  @id @default(uuid())
  username  String  @unique
  password  String
  cardId    String  @unique @db.VarChar(13)
  mobile    String  @unique @db.VarChar(10)
  status    Boolean


}
